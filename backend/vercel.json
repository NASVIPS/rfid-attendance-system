{
       "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node",
      "config": {
        "builds": [
          {
            "src": "src/app.js",
            "use": "@vercel/node"
          }
        ],
        "buildCommand": "npx prisma generate"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ]
}